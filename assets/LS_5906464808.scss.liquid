//Search page styles
#LS_find_result_summary {
    display: none;
    text-align: center;
    margin-bottom: 12px;
}

#LS_search_form {
    max-width: 50%;
    min-width: 250px;
    margin: 0 auto;

    form {
        display: block;
        margin-bottom: 14px;
    }

    input {
        border: 1px solid #ebebeb;
        width: 100%;
        max-width: 100%;
        display: block;
        margin: 0 0 2.4em;
        background-color: transparent;
        -webkit-transition: border-color 0.2s ease-out;
        -moz-transition: border-color 0.2s ease-out;
        -ms-transition: border-color 0.2s ease-out;
        -o-transition: border-color 0.2s ease-out;
        transition: border-color 0.2s ease-out;
        &:hover, &:focus {
            border: 1px solid #c5c5c5;
        }
    }
}

#LS_find_result_grid {

    font-size: 0;
    text-align: center;

    p {
        padding: 0 10px;
        font-size: 1.05em;
    }


    a {
        cursor: pointer;
        text-decoration: none;
        border-bottom: none;
        font-size: 1.1em;
        line-height: 1.2em;
    }

    &:after {
        clear: both;
        content: "";
        display: block;
    }

    &.ls_searching_progress {

        position: relative;
        min-height: 100px;
        visibility: visible;
        opacity: 1;

        &:before {
            content: "";
            top: 0;
            left: 0;
            display: block;
            width: 100%;
            height: 100%;
            position: absolute;
            background: rgba(255,255,255,0.3);
            z-index: 1;
        }

        &:after {
            content: "";
            position: absolute;
            width: 50px;
            height: 50px;
            top: 50%;
            left: 50%;
            margin: -25px 0 0 -25px;
            background-size: 50px 50px;
            z-index: 2;
            background-image: url(data:image/png;base64,);
            -webkit-animation: ls-search-rotation 1s infinite linear;
            -moz-animation: ls-search-rotation 1s infinite linear;
            animation: ls-search-rotation 1s infinite linear;
        }
    }


    .ls_grid_item {
        font-size: 14px;
        display: inline-block;
        text-decoration: none;
        margin: 25px 0.5%;
        width: 32%;
        vertical-align: top;
        text-align: center;
        min-width: 180px;
    }

    .ls_title {
        padding: 0 8%;
        font-size: 1.1em;
        line-height: 1.6;
    }

    .ls_product_price {
        font-size: 1.1em;
        margin-bottom: 0;
    }

    .ls_product_image_wrapper {
        display: table;
        table-layout: fixed;
        margin: 0 auto 12.5px;
        position: relative;
        padding: 0 8%;
        width: 100%;
        height: auto;

        &:hover {
            img {
                opacity: 0.6;
            }
        }

        a {
            display: table-cell;
            vertical-align: middle;
        }

        img {
            display: block;
            margin: 0 auto;
            opacity: 1;
            -webkit-transition: opacity 0.3s ease-out;
            -moz-transition: opacity 0.3s ease-out;
            -ms-transition: opacity 0.3s ease-out;
            -o-transition: opacity 0.3s ease-out;
            transition: opacity 0.3s ease-out;
            -webkit-backface-visibility: hidden;
            -moz-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            backface-visibility: hidden;
            max-width: 100%;
        }
    }
}


//Search popup styles
#LS_search_popup {

    position: fixed;
    z-index: 9999999;
    visibility: hidden;
    opacity: 0;
    border: 1px solid #ebebeb;
    box-sizing: border-box;
    background: #fff;
    overflow: auto;

    p {
        padding: 2px 0;
        font-size: 1.05em;
    }

    h3 {
        padding: 7px 0;
        margin: 0;
        font-size: 1.2em;
    }

    a {
        cursor: pointer;
        text-decoration: none;
        border-bottom: none;
        font-size: 1.1em;
        line-height: 1.2em;
    }

    &.active {
        visibility: visible;
        opacity: 1;
    }

    &.ls_searching_progress {

        min-height: 100px;
        visibility: visible;
        opacity: 1;

        &:before {
            content: "";
            top: 0;
            left: 0;
            display: block;
            width: 100%;
            height: 100%;
            position: absolute;
            background: rgba(255,255,255,0.3);
            z-index: 1;
        }

        &:after {
            content: "";
            position: absolute;
            width: 50px;
            height: 50px;
            top: 50%;
            left: 50%;
            margin: -25px 0 0 -25px;
            background-size: 50px 50px;
            z-index: 2;
            background-image: url(data:image/png;base64,);
            -webkit-animation: ls-search-rotation 1s infinite linear;
            -moz-animation: ls-search-rotation 1s infinite linear;
            animation: ls-search-rotation 1s infinite linear;
        }
    }

    .ls_search_popup_wrapper {
        position: relative;
    }

    .ls_grid_item {
        margin: 0 auto 12.5px;
        width: 100%;
    }

    .ls_grid_product {
        display: table;
        position: relative;
        width: 100%;
        height: auto;
        & > * {
            display: table-cell;
            margin: 0;
            line-height: 1;
            vertical-align: middle;
            padding: 3px;
        }
    }

    .ls_product_image_wrapper {
        width: 70px;
        &:hover {
            img {
                opacity: 0.6;
            }
        }
        img {
            display: block;
            margin: 0 auto;
            opacity: 1;
            -webkit-transition: opacity 0.3s ease-out;
            -moz-transition: opacity 0.3s ease-out;
            -ms-transition: opacity 0.3s ease-out;
            -o-transition: opacity 0.3s ease-out;
            transition: opacity 0.3s ease-out;
            -webkit-backface-visibility: hidden;
            -moz-backface-visibility: hidden;
            -ms-backface-visibility: hidden;
            -o-backface-visibility: hidden;
            backface-visibility: hidden;
            max-width: 100%;
        }
    }
}

#LS_search_popup_summary {
    box-sizing: border-box;
    padding: 5px 10px;
    display: none;
}

#LS_search_popup_products {
    box-sizing: border-box;
    padding: 5px 10px;
    display: none;
    border-top: 1px solid #ebebeb;
}

#LS_search_popup_articles {
    box-sizing: border-box;
    padding: 5px 10px;
    display: none;
    border-top: 1px solid #ebebeb;
}

#LS_search_popup_collections {
    box-sizing: border-box;
    padding: 5px 10px;
    display: none;
    border-top: 1px solid #ebebeb;
}

@-webkit-keyframes ls-search-rotation {
    from {-webkit-transform: rotate(0deg);}
    to   {-webkit-transform: rotate(360deg);}
}

@-moz-keyframes ls-search-rotation {
    from {-webkit-transform: rotate(0deg);}
    to   {-webkit-transform: rotate(360deg);}
}

@keyframes ls-search-rotation {
    from {transform: rotate(0deg);}
    to   {transform: rotate(360deg);}
}


//Default currency popup
.LS_converted_currencies {
    display: none;
    &.LS_show_on_hover {
        display: inline-block;
        visibility: hidden;
        vertical-align: middle;
        line-height: 100%;
        opacity: 0;
        width: 0;
        transition: visibility 0.3s linear 0.5s, opacity 0.5s linear;
        &:after, &:before {
            visibility: hidden;
            opacity: 0;
            transition: visibility 0.3s linear 0.5s, opacity 0.5s linear;
        }
    }
}
body *:hover {
    & > .LS_converted_currencies.LS_show_on_hover {
        position: relative;
        visibility: visible;
        opacity: 1;
        transition: visibility 0.3s linear 0.5s, opacity 0.5s linear;
        &:after {
            content: attr(data-default);
            display: inline-block;
            position: absolute;
            width: auto;
            background: rgba(0,0,0,0.8);
            color: #fff;
            padding: 5px;
            border-radius: 7px;
            font-size: 14px;
            line-height: 1;
            word-wrap: normal;
            word-break: normal;
            top: calc(50% - 12px);
            left: 7px;
            white-space: nowrap;
            visibility: visible;
            opacity: 1;
            font-weight: normal;
            font-family: Helvetica Neue,Arial,sans-serif;
            text-decoration: none;
            transition: visibility 0.3s linear 0.5s, opacity 0.5s linear;
            z-index: 9;
        }
        &:before {
            content: "";
            display: inline-block;
            position: absolute;
            width: auto;
            background: transparent;
            color: #fff;
            border: none;
            border-top: 5px solid transparent;
            border-bottom: 5px solid transparent;
            border-right: 5px solid rgba(0,0,0,0.8);
            top: calc(50% - 5px);
            left: 2px;
            visibility: visible;
            opacity: 1;
            transition: visibility 0.3s linear 0.5s, opacity 0.5s linear;
        }
    }
}

html[dir=rtl] {
    body *:hover {
        & > .LS_converted_currencies.LS_show_on_hover {

            &:after {
                right: 7px;
                left: auto;
            }
            &:before {
                right: 2px;
                left: auto;
                transform: rotate(180deg);
            }
        }
    }
}


//Switcher themes

/* Style settings for Theme B */
$theme-b-active-bg-color: #DE4C39;
$theme-b-active-font-color: #FFFFFF;

/* Style settings for Theme A */
$theme-a-active-font-color: #555;
$theme-a-active-bg-color: #FEFEFE;
$theme-a-bg-color: #FFFFFF;
$theme-a-bg-hover-color: #F6F6F6;
$theme-a-font-color: #403F3F;

[id^="ls-lang-switcher-"], [id^="ls-curr-switcher-"] {
    z-index: 999;
    display: inline-block;
}

/* Styles sets */
.ls-style-theme-a {

    direction: ltr;
    display: inline-block;
    z-index: 999;
    position: relative;
    font-family: "Helvetica Neue", Arial, sans-serif;

    .flag-wrap {
        margin-right: 5px;
        margin-left: 3px;
        cursor: pointer;
        overflow: hidden;
        display: inline-block;
        vertical-align: middle;
        line-height: 1;
        width: auto;
        height: 32px;
        box-sizing: border-box;
        padding: 4px;

        img {
            vertical-align: middle;
            width: auto;
            height: 100%;
            border-radius: 4px;
        }
    }

    .ls-li-vertical, .ls-li-vertical-non-text, .ls-li-vertical-non-flags {

        position: relative;
        display: inline-block;
        color: $theme-a-font-color;
        background-color: $theme-a-active-bg-color;
        border-radius: 5px;

        a {
            color: inherit;
            cursor: pointer;
        }

        .ls-curr-switcher-active, .ls-lang-switcher-active {
            font-weight: bold;
            padding: 0 22px 0 3px;
            border-radius: 4px;
            white-space: nowrap;
            cursor: pointer;
            color: $theme-a-active-font-color;
            display: inline-block;
            vertical-align: middle;
            line-height: 28px;
            background-color: #ffffff;
            border: 1px solid #dadada;
            font-size: 15px;
        }

        .ls-curr-switcher-active:after, .ls-lang-switcher-active:after {
            border-bottom: 2px solid $theme-a-active-font-color;
            border-right: 2px solid $theme-a-active-font-color;
            content: "";
            display: block;
            height: 5px;
            font-weight: bold;
            box-sizing: content-box;
            pointer-events: none;
            position: absolute;
            right: 10px;
            -webkit-transform-origin: 66% 66%;
            transform-origin: 66% 66%;
            -webkit-transform: rotate(45deg);
            transform: rotate(45deg);
            transition: all .15s ease-in-out;
            width: 5px;
            top: 0;
            bottom: 3px;
            margin: auto;
        }

        ul {
            background-color: $theme-a-bg-color;
            margin: 0;
            padding: 0;
            border-radius: 5px;
            box-sizing: border-box;
            opacity: 0;
            visibility: hidden;
            overflow: auto;
            overflow-x: hidden;
            pointer-events: none;
            position: absolute;
            top: 100%;
            max-height: 260px;
            -webkit-transform-origin: 50% 0;
            transform-origin: 50% 0;
            -webkit-transform: scale(.75) translateY(-21px);
            transform: scale(.75) translateY(-21px);
            transition: all .2s cubic-bezier(.5,0,0,1.25),opacity .15s ease-out;
            z-index: 100000;
            list-style: none;
            color: $theme-a-font-color;

            li {
                white-space: nowrap;
                font-size: 13px !important;
                float: none!important;
                text-align: left !important;
                margin: 0!important;
                font-family: "Helvetica Neue", Arial, sans-serif !important;
                letter-spacing: normal;
                text-transform: none;
                display: block!important;
                cursor: pointer;
                font-weight: 400;
                line-height: 38px!important;
                list-style: none;
                min-height: 40px;
                min-width: 38px;
                margin-bottom: 0;
                box-sizing: content-box;
                outline: 0;
                padding-left: 18px;
                padding-right: 75px;
                transition: all .2s;
                background-color: transparent;
                &:first-child {
                    border-radius: 5px 5px 0 0;
                }
                &:last-child {
                    border-radius: 0 0 5px 5px;
                }

                &:hover {
                    background-color: $theme-a-bg-hover-color;
                }
            }
        }

        &.opened {

            .ls-curr-switcher-active:after, .ls-lang-switcher-active:after {
                -webkit-transform: rotate(-135deg);
                transform: rotate(-135deg);
            }

            & > ul {
                opacity: 1;
                visibility: visible;
                pointer-events: auto;
                -webkit-transform: scale(1) translateY(0);
                transform: scale(1) translateY(0);
                z-index: 1000000 !important;
            }
        }
    }


    .ls-li-vertical-non-text li {
        padding-right: 18px!important;
    }


    .ls-li-horizontal, .ls-li-horizontal-non-text, .ls-li-horizontal-non-flags {

        background-color: $theme-a-bg-color;
        color: $theme-a-font-color;
        border-radius: 5px;
        padding: 0;
        margin: 0;
        list-style: none;

        a {
            color: inherit;
            cursor: pointer;
        }

        .ls-curr-switcher-active, .ls-lang-switcher-active {
            font-weight: bold;
            box-sizing: border-box;
            color: $theme-a-active-font-color;
            background: $theme-a-active-bg-color;
            margin: 5px;
        }

        li {
            cursor: pointer;
            line-height: 28px;
            vertical-align: middle;
            margin: 0 5px 0 5px;
            padding: 3px;
            list-style-type: none;
            display: inline-block;
            white-space: nowrap;
            background-color: transparent;
            border-radius: 5px;
            &:not(.ls-curr-switcher-active):not(.ls-lang-switcher-active):hover {
                background-color: $theme-a-bg-hover-color;
            }
        }

    }

}

.ls-style-theme-b {

    display: inline-block;
    z-index: 999;
    position: relative;
    direction: ltr;
    font-family: "Helvetica Neue", Arial, sans-serif;

    .flag-wrap {
        margin-right: 5px;
        margin-left: 3px;
        cursor: pointer;
        overflow: hidden;
        display: inline-block;
        vertical-align: middle;
        line-height: 1;
        width: auto;
        height: 32px;
        box-sizing: border-box;
        padding: 4px;

        img {
            vertical-align: middle;
            width: auto;
            height: 100%;
            border-radius: 10px;
        }
    }

    .ls-li-horizontal, .ls-li-horizontal-non-text, .ls-li-horizontal-non-flags {

        padding: 0 0 0 50px;
        background-color: transparent;
        white-space: nowrap;
        margin: 0;
        list-style: none;

        a {
            color: inherit;
            cursor: pointer;
        }

        .ls-curr-switcher-active, .ls-lang-switcher-active {
            max-width: none;
            border: 1px solid $theme-b-active-bg-color;
            box-sizing: border-box;
            text-overflow: inherit;
            background: $theme-b-active-bg-color;
            padding: 1px 12px 1px 12px;
            z-index: 1000000!important;
            color: $theme-b-active-font-color;
            font-weight: bold;
            direction: ltr!important;
            min-width: 130px;
        }

        li {
            cursor: pointer;
            border-radius: 25px;
            text-align: center;
            vertical-align: middle;
            list-style-type: none;
            display: inline-block;
            white-space: nowrap;
            margin: 0;
            margin-left: -46px;
            position: relative;
            padding: 1px 12px 1px 12px;
            background: #FFFFFF;
            border: 1px solid #e8e7e7;
            color: #adabab;
            max-width: 90px;
            overflow: hidden;
            font-size: 15px;
            box-sizing: border-box;
            &:not(.ls-curr-switcher-active):not(.ls-lang-switcher-active):hover {
                background: #DDF6CF;
                border-color: #89B171;
                color: #89B171;
            }
        }

    }
}