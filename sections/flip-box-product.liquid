<div id="section_flip_box"> 
            <div class="feature-row">


                <div class="feature-row__item feature-row__item_mobile_half custom_row1 flip_row ">
                        <div class="flip_content_text">
                                NEW  RELEASES
                        </div>
                </div>
                {% for product in collections.home-flip.products  %}
             
                <div class="feature-row__item feature-row__item_mobile_half flip_row ">
                  <a href="{{ product.url }}" >
                        <div class="flip_box">
                        <div class="flip_box_inner">
                            <div class="flip_img1 flip_img">
                            {% assign i = 1 %}
                            {% for image in product.images %}
                               {% assign i = i | plus: 1 %}
                              {% comment %} {% if i == 1 %}
                       
                             <img src=" {{ image | img_url: 'medium' }}" alt="{{ product.title | escape  }}" class="new-product-image"  />

                             {% endif %}
                            {% if i == 2 %}
                             <img src=" {{ image | img_url: 'medium' }}" alt="{{ product.title | escape  }}" class="new-product-image_back"  />
                    
                            {%else%}
                             {% break %}
                             {% endif %}
                             {% endcomment %}
                             {% if i == 2 %}
                              <img src=" {{ image | img_url: 'large' }}" alt="{{ product.title | escape  }}" class="new-product-image"  />
                             
                             {% endif %}
                               {% if i == 3 %}
                              <img src=" {{ image | img_url: 'large' }}" alt="{{ product.title | escape  }}" class="new-product-image_back"  />                            
                             {% endif %}

                            
                      
                           {% endfor %}
                            {% comment %} <img src="{{ product.images.first | img_url: 'large' }}" alt="{{ product.title | escape  }}" class="new-product-image"  /> {% endcomment %}
                            {% comment %} <img src="{{ product.images.second | img_url: 'large' }}" alt="{{ product.title | escape  }}" class="new-product-image_back"  /> {% endcomment %}

                            {% comment %} <img src="https://image.ibb.co/kUcqhf/vital-seamless-4.jpg" class="new-product-image" alt="clothes">
                            <img src="https://image.ibb.co/cMk5oA/vital-pink-seamlesss-leggings-womens-4.jpg" class="new-product-image_back" alt="clothes">                             {% endcomment %}
                            </div>
                           
                         </div>
                         </div>
                        <p>{{ product.title }}<br>{{ product.price | money }}</p>
                        <p>
                                {% comment %} {% for variant in product.variants %}
                                {{ variant.title }}
                                {% endfor %} {% endcomment %}
                            
                       </p>
                                                 
                    </a>      
                </div>
               
                {% endfor %}
             
               
        
     </div>
</div>
{% comment %} {% for product in collection.all_products %}
    {% if product.handle contains 'neo-hoodie' %}
      {{true}}
        <img src="{{ collection | img_url: 'large' }}" alt="{{ collection.image.alt }}" />
      {% else %}
    {% endif %}
{% endfor %} {% endcomment %}

